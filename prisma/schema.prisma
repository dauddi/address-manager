generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int       @id @default(autoincrement())
  firstName String
  lastName  String
  email     String
  password  String
  contacts  Contact[]
}

model Contact {
  id          Int    @id @default(autoincrement())
  name        String
  phoneNumber String
  address     String
  creator     User   @relation(fields: [creatorId], references: [id])
  creatorId   Int
}
